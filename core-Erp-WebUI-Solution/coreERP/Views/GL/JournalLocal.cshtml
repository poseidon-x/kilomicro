
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "savings Withdrawal";
}

<h2>Journal Entry Transactions</h2>

<div id="myform">
    <div id="form-container" style="position: relative">
        <div class="form-horizontal">
           <div class="row">
                <div class="col-md-2 col-xs-2">Transaction Date: </div>
                <div class="col-md-4 col-xs-4">
                    <input type="text" id="availablePrincipalBalance" readonly/>
                </div>
                <div class="col-md-2 col-xs-2">Batch Originator: </div>
                <div class="col-md-4 col-xs-4">
                    <input type="text" id="interestBalance" readonly />
                </div>
            </div><br />
            <div class="row">
                <div class="col-md-2 col-xs-2">Batch Number: </div>
                <div class="col-md-4 col-xs-4">
                    <input type="text" id="batchNumber" readonly />
                </div>
                <div class="col-md-2 col-xs-2">Batch State: </div>
                <div class="col-md-4 col-xs-4">
                    <input type="text" id="batchState" readonly />
                </div>
            </div><br />
        </div>
        <br/>
        <div id="row">
            <div id="tabs">
                <ul>
                    <li class="k-state-active">Transactions</li>
                </ul>
                <div>
                    <div id="transactions"></div>
                </div>
               </div> <!--EndTab--><br />
        </div>

        <!--<div class="row">
            <div class="col-md-2">
                <button type="button" class="btn btn-primary" id="save">Save Withdrawal &raquo; </button>
            </div>
            <br />
        </div> -->
        <br />
    </div>
</div>

@section FootScripts{
    <script type="text/javascript">
        var id = '@ViewBag.Id';
    </script>
    
    <script src="~/scripts/app/GL/journalLocal.js"></script>
}