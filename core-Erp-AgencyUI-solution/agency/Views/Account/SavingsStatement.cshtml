@{
    ViewBag.Title = "Savings Statement";
}
@section HeadScripts{
    <style>
        #reportViewer1 {
            position: absolute;
            left: 80px;
            right: 80px;
            top: 200px;
            height: 100%;
            font-family: 'segoe ui', 'ms sans serif';
            overflow: hidden;
        }
    </style>
}
<hr/>

<div id="myform" class="w-section inverse 1w boxed container">
    <div class="row">
        <div class="col-md-1 col-xs-1 col-lg-1">Account</div>
        <div class="col-md-3 col-xs-3 col-lg-3">
            <input type="date" id="saving"/>
        </div>
        <div class="col-md-1 col-xs-1 col-lg-1">Start Date</div>
        <div class="col-md-3 col-xs-3 col-lg-3">
            <input type="date" id="startDate"/>
        </div>
        <div class="col-md-1 col-xs-1 col-lg-1">End Date</div>
        <div class="col-md-3 col-xs-3 col-lg-3">
            <input type="date" id="endDate"/>
        </div>
    </div>
    <div class="row">
        <div class="k-widget" id="reportViewer1"></div>
    </div>
</div>


    @section FootScripts
{
        <script src="/Content/reporting/js/telerikReportViewer-10.0.16.113.min.js"></script>
        <script src="~/scripts/app/Loans/savings/SavingsReport/SavingsStatement.js"></script>
    }
